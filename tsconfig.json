{
    "compilerOptions": {
        "outDir": "./dist",
        "target": "es5",
        "lib": ["esnext", "dom"],
        "jsx": "react",
        "reactNamespace": "JSX",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "downlevelIteration": true
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}
